  "devDependencies": {
    "@babel/cli": "7.5.0",
    "@babel/core": "7.5.0",
    "@babel/preset-env": "7.5.3",
    "@babel/register": "7.4.4",
    "assert": "1.4.1",
    "documentation": "8.0.2",
    "lodash": "4.17.11",
    "mocha": "5.2.0"
